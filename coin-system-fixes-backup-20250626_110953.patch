+<script>
+// CRITICAL FIX: Synchronized Spin Animation System
+const rewards = <?php echo json_encode(array_values($specific_rewards)); ?>;
+const selectedRewardIndex = <?php echo $selected_reward_index ?? 'null'; ?>;
+const serverSpinAngle = <?php echo $spin_angle ?? 'null'; ?>;
+
+console.log('🎯 SPIN WHEEL SYNC SYSTEM ACTIVE');
+console.log('Backend selected prize index:', selectedRewardIndex);
+console.log('Backend calculated angle:', serverSpinAngle);
+console.log('Frontend rewards array:', rewards);
+
+// Fixed Animation Function - Uses Backend Result
+function animateSpinWheel() {
+    if (selectedRewardIndex === null || serverSpinAngle === null) {
+        console.error('❌ No backend result to animate to!');
+        return;
+    }
+    
+    console.log('🎬 Animating to backend-selected winner:', rewards[selectedRewardIndex].name);
+    
+    const canvas = document.getElementById('spin-canvas');
+    const ctx = canvas.getContext('2d');
+    let currentAngle = 0;
+    const targetAngle = serverSpinAngle;
+    const duration = 4000; // 4 second spin
+    const startTime = Date.now();
+    
+    function animate() {
+        const elapsed = Date.now() - startTime;
+        const progress = Math.min(elapsed / duration, 1);
+        
+        // Easing function for realistic deceleration
+        const easeOut = 1 - Math.pow(1 - progress, 3);
+        currentAngle = targetAngle * easeOut;
+        
+        // Draw wheel at current angle
+        drawWheelAtAngle(ctx, currentAngle);
+        
+        if (progress < 1) {
+            requestAnimationFrame(animate);
+        } else {
+            // Animation complete - show the predetermined winner
+            console.log('✅ Animation complete, showing predetermined winner');
+            showWinPopup(
+                rewards[selectedRewardIndex].name, 
+                rewards[selectedRewardIndex].points, 
+                <?php echo $user_points + ($selected_reward['points'] ?? 0); ?>
+            );
+        }
+    }
+    
+    animate();
+}
+
+// Enhanced Win Popup System
+function showWinPopup(prizeName, prizeAmount, newBalance) {
+    console.log('🎉 Showing win popup for CONFIRMED backend result:', prizeName, prizeAmount, newBalance);
+    
+    // Update modal content
+    const winAmount = document.getElementById('winAmount');
+    const winDescription = document.getElementById('winDescription');
+    const newBalanceSpan = document.getElementById('newBalance');
+    
+    if (winAmount) {
+        if (prizeAmount > 0) {
+            winAmount.innerHTML = `🎉 YOU WON ${prizeAmount} COINS! 🎉`;
+            winDescription.innerHTML = `${prizeName} prize added to your balance!`;
+        } else if (prizeAmount < 0) {
+            winAmount.innerHTML = `😱 ${prizeName}`;
+            winDescription.innerHTML = `You lost ${Math.abs(prizeAmount)} coins, but gained experience!`;
+        } else {
+            winAmount.innerHTML = `🎊 YOU WON: ${prizeName}! 🎊`;
+            winDescription.innerHTML = 'Special prize unlocked!';
+        }
+    }
+    
+    if (newBalanceSpan) {
+        newBalanceSpan.textContent = newBalance.toLocaleString();
+    }
+    
+    // Show modal with animation
+    const winModal = new bootstrap.Modal(document.getElementById('winModal'), {
+        keyboard: false,
+        backdrop: 'static'
+    });
+    
+    // Add celebration sound effect (if available)
+    try {
+        const audio = new Audio('../assets/sounds/win.mp3');
+        audio.volume = 0.3;
+        audio.play().catch(() => console.log('Win sound not available'));
+    } catch (e) {
+        console.log('Win sound effect not available');
+    }
+    
+    winModal.show();
+    
+    // Auto-dismiss after 5 seconds
+    setTimeout(() => {
+        winModal.hide();
+    }, 5000);
+}
+
+// Auto-show popup on page load if there's a win message
+document.addEventListener('DOMContentLoaded', function() {
+    // Check if there's a PHP message indicating a win
+    const messageElement = document.getElementById('spin-message');
+    if (messageElement && messageElement.textContent.includes('won')) {
+        // Extract prize info from message
+        const messageText = messageElement.textContent;
+        let prizeAmount = 0;
+        let prizeName = 'Unknown Prize';
+        
+        // Parse different prize formats
+        const coinMatch = messageText.match(/(\d+)\s*(?:QR\s*)?[Cc]oins?/);
+        if (coinMatch) {
+            prizeAmount = parseInt(coinMatch[1]);
+            prizeName = `${prizeAmount} QR Coins`;
+        } else {
+            // Extract prize name from "You won: X" format
+            const prizeMatch = messageText.match(/won:?\s*(.+?)(?:\s*\+|$)/i);
+            if (prizeMatch) {
+                prizeName = prizeMatch[1].trim();
+            }
+        }
